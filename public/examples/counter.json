{
  "name": "Counter",
  "description": "A simple standard counter example",
  "files": [
    {
      "name": "counter",
      "content": [
        "import { createState, onCleanup } from \"solid-js\";",
        "import { render } from \"solid-js/web\";",
        "",
        "const CountingComponent = () => {",
        "\tconst [state, setState] = createState({ counter: 0 });",
        "\tconst interval = setInterval(",
        "\t\t() => setState({ counter: state.counter + 1 }),",
        "\t\t1000",
        "\t);",
        "\tonCleanup(() => clearInterval(interval));",
        "\treturn <div>Count value is {state.counter}</div>;",
        "};",
        "",
        "render(() => <CountingComponent />, document.getElementById(\"app\"));"
      ]
    }
  ]
}
